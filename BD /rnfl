-- Table: actividad_finalizada

-- DROP TABLE actividad_finalizada;

CREATE TABLE actividad_finalizada
(
  id bigserial NOT NULL,
  id_actividad integer,
  id_feria integer,
  nombre_responsable character varying,
  fecha_ejecucion date,
  hora_ejecucion time without time zone,
  hora_fin_ejecucion time without time zone,
  participantes_m integer,
  participantes_f integer,
  total integer,
  evento_publico boolean,
  otro_incumplimiento character varying,
  nombre_institucion character varying,
  id_pais integer DEFAULT 1,
  id_estado integer DEFAULT 1,
  id_municipio integer DEFAULT 1,
  id_parroquia integer DEFAULT 1,
  id_region integer DEFAULT 1,
  incluir_info_geografica boolean,
  CONSTRAINT actividad_finalizada_pkey PRIMARY KEY (id ),
  CONSTRAINT actividad_finalizada_id_actividad_fkey FOREIGN KEY (id_actividad)
      REFERENCES actividad (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_estado_fkey FOREIGN KEY (id_estado)
      REFERENCES estado (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_feria_fkey FOREIGN KEY (id_feria)
      REFERENCES feria (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_municipio_fkey FOREIGN KEY (id_municipio)
      REFERENCES municipio (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_pais_fkey FOREIGN KEY (id_pais)
      REFERENCES pais (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_parroquia_fkey FOREIGN KEY (id_parroquia)
      REFERENCES parroquia (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT actividad_finalizada_id_region_fkey FOREIGN KEY (id_region)
      REFERENCES region (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)
WITH (
  OIDS=FALSE
);
